    "uuid": "^8.3.2",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.2",
    "@types/aws-lambda": "^8.10.101",
    "@types/aws4": "^1.11.2",
    "@types/http-errors": "^1.8.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^8.3.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.15.15",
    "esbuild-visualizer": "^0.3.1",
