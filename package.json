    "@middy/validator": "^3.1.0",
    "ajv": "^8.12.0",
    "amazon-cognito-identity-js": "^6.3.1",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1168.0",
    "aws4": "^1.11.0",
    "aws4-axios": "^3.2.17",
    "axios": "^1.3.6",
    "csv-parse": "^5.4.0",
    "csvtojson": "^2.0.10",
    "dynamodb-data-types": "^4.0.0",
    "fast-xml-parser": "^4.0.10",
    "form-data": "^4.0.0",
    "http-errors": "^2.0.0",
    "js-base64": "^3.7.3",
    "json-schema-to-ts": "^2.5.4",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.2",
