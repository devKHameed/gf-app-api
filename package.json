{
  "name": "gf-app-backend-serverless",
  "version": "1.0.0",
  "description": "GF application serverless backend",
  "main": "index.js",
  "scripts": {
    "start": "cross-env MAIN_POOL=public-1 ACCT_NAME=main sls offline start",
    "debug": "SET SLS_DEBUG=* && node --inspect node_modules\\serverless\\bin\\serverless offline",
    "deploy": "sls deploy",
    "lint:fix": "eslint --fix --quiet",
    "prettier:fix": "yarn prettier --write",
    "prepare": "husky install",
    "lint": "eslint src/",
    "deploy:fusion": "cross-env ACCT_NAME=fusion sls deploy",
    "deploy:main": "cross-env ACCT_NAME=main sls deploy",
    "deploy:main:data-mng": "cross-env MAIN_POOL=data-mng ACCT_NAME=main sls deploy",
    "deploy:main:user-mng": "cross-env MAIN_POOL=user-mng ACCT_NAME=main sls deploy",
    "deploy:main:dev-sett": "cross-env MAIN_POOL=dev-sett ACCT_NAME=main sls deploy",
    "deploy:main:gui-fusion": "cross-env MAIN_POOL=gui-fusion ACCT_NAME=main sls deploy",
    "deploy:main:public-1": "cross-env MAIN_POOL=public-1 ACCT_NAME=main sls deploy",
    "package:fusion": "cross-env ACCT_NAME=fusion sls package",
    "package:main": "cross-env  ACCT_NAME=main sls package",
    "package:main:data-mng": "cross-env MAIN_POOL=data-mng ACCT_NAME=main sls package",
    "package:main:public-1": "cross-env MAIN_POOL=public-1 ACCT_NAME=main sls package",
    "print:fusion": "cross-env ACCT_NAME=fusion sls print",
    "print:main": "cross-env ACCT_NAME=main sls print",
    "analyze:main": "cross-env ACCT_NAME=main sls package --analyze",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devKHameed/gf-app-api"
  },
  "keywords": [
    "gf-app"
  ],
  "author": "Khalid Hameed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devKHameed/gf-app-api/issues"
  },
  "homepage": "https://github.com/devKHameed/gf-app-api#readme",
  "dependencies": {
    "@aws-sdk/util-dynamodb": "^3.172.0",
    "@maxmind/geoip2-node": "^3.5.0",
    "@middy/core": "^3.1.0",
    "@middy/do-not-wait-for-empty-event-loop": "^3.1.0",
    "@middy/http-error-handler": "^3.1.0",
    "@middy/http-event-normalizer": "^3.1.0",
    "@middy/http-json-body-parser": "^3.1.0",
