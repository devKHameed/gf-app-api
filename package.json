    "serverless-analyze-bundle-plugin": "^1.2.1",
    "serverless-auto-swagger2.0": "^2.8.3",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.33.2",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^9.0.0",
    "serverless-plugin-split-stacks": "^1.11.3",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-webpack": "^5.8.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ]
  }
}
